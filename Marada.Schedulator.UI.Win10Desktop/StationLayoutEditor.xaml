<Window x:Class="Marada.Schedulator.UI.Win10Desktop.StationLayoutEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Marada.Schedulator.UI.Win10Desktop"
        mc:Ignorable="d"
        Title="Stationslayout"
        Height="600"
        Width="800">
    <Grid>
        <Canvas>
            <Line Stroke="Black"
                  StrokeThickness="4"
                  X1="100"
                  Y1="300"
                  X2="500"
                  Y2="300" />
            <Ellipse Height="20"
                     Width="20"
                     Fill="White"
                     Stroke="Black"
                     StrokeThickness="5"
                     Margin="200,290" />
            <Line Stroke="Black"
                  StrokeThickness="4"
                  X1="200"
                  Y1="300"
                  X2="300"
                  Y2="200" />
        </Canvas>
    </Grid>
</Window>
